@model IEnumerable<S1.Models.Auta>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <b>@Html.Label("Zserializowane Dane")</b>
    <p >
        @*@ViewBag.Serializacja*@

        <table border="1" width="500" cellspacing="0" cellpadding="0">
            <tr>
                <td>@ViewBag.AutoNr1</td>
            </tr>
            <tr>
                <td>@ViewBag.AutoNr2</td>
            </tr>
            <tr>
                <td>@ViewBag.AutoNr3</td>
            </tr>
        </table>
    </p>
    <b>Zdeserializowane Dane</b>
    <table border="1" width="500" cellspacing="0" >
        <tr>
            <th width="150" align="center">
                @Html.DisplayNameFor(model => model.Marka)
            </th>
            <th width="150" align="center">
                @Html.DisplayNameFor(model => model.Model)
            </th>
            <th width="150" align="center">
                @*@Html.DisplayNameFor(model => model.Nadwozie) |*@
                @Html.Label("Nadwozie")
            </th>
            <th width="150" align="center">
                @*@Html.DisplayNameFor(model => model.Rok_Produkcji)*@
                @Html.Label("Rok Produkcji")
            </th>
            <th width="150" align="center">
                @*@Html.DisplayNameFor(model => model.Pojemnosc_Silnika)*@
                @Html.Label("Pojemność Silnika")
            </th>
            <th width="150" align="center">
                @Html.DisplayNameFor(model => model.Paliwo)
            </th>
            <th width="150" align="center">
                @Html.DisplayNameFor(model => model.Cena)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Marka)
                </td>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Model)
                </td>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Nadwozie)
                </td>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Rok_Produkcji)
                </td>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Pojemnosc_Silnika)
                </td>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Paliwo)
                </td>
                <td align="center">
                    @Html.DisplayFor(modelItem => item.Cena)
                </td>
                @*<td>
                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                    </td>*@
            </tr>
        }

    </table>
</body>
</html>
